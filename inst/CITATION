citHeader("To cite omnivor in publications use:")

citEntry(
  entry    = "Manual",
  title    = "omnivor - Ecological Networks Analysis",
  author   = "Clément Violet and Dominique Gravel",
  year     = "2019",
  note     = "R package version 0.0.0.950",
  url      = "https://github.com/clementviolet/omnivor",
  textVersion = paste("Clément Violet, Dominique Gravel (2019).",
                      "omnivor - Ecological Networks Analysis.",
                      "R package version 0.0.0.950.",
                      "https://github.com/clementviolet/omnivor"
  )
)
